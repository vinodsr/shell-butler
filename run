BASEDIR=$(dirname $0)
if [ "$1" = "add" ]; then
    shell-butler $*
else

    result=$(shell-butler $*)
    ret=$?

    echo "\U1F916 Hey"
    if [ $ret -eq 0 ]; then
        echo
        echo "\U1F3C3 : $result"
        echo
        eval $result
    else
        echo
        echo "\U2757 You haven't selected any command"
        echo
    fi
fi
